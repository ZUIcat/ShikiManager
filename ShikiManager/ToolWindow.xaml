<Window x:Class="ShikiManager.ToolWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShikiManager"
        mc:Ignorable="d"
        Title="ToolWindow" Height="50" Width="500" WindowStyle="None" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="Manual" SizeToContent="WidthAndHeight" ShowInTaskbar="False" Loaded="OnToolWindowLoaded">
    <StackPanel Orientation="Horizontal">
        <Button x:Name="Button001" MinWidth="50" MinHeight="50" Click="OnButton001Click">截屏</Button>
        <Button x:Name="Button002" MinWidth="50" MinHeight="50" Click="OnButton002Click">Ctrl</Button>
        <Button x:Name="Button003" MinWidth="50" MinHeight="50" Click="OnButton003Click">Space</Button>
        <Button x:Name="Button004" MinWidth="50" MinHeight="50" Click="OnButton004Click">KeyUp</Button>
        <Button x:Name="Button005" MinWidth="50" MinHeight="50" Click="OnButton005Click">KeyDown</Button>
        <Button x:Name="Button006" MinWidth="50" MinHeight="50" Click="OnButton006Click">ScrollUp</Button>
        <Button x:Name="Button007" MinWidth="50" MinHeight="50" Click="OnButton007Click">ScrollDown</Button>
    </StackPanel>
</Window>
