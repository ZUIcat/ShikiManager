    <!--// Side Black Button //-->
    <SolidColorBrush x:Key="Button_Style_Icon_01.Static.Foreground" Color="{StaticResource Gray_001}"/>
    <SolidColorBrush x:Key="Button_Style_Icon_01.Static.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="Button_Style_Icon_01.Static.BorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="Button_Style_Icon_01.MouseOver.Foreground" Color="White"/>
    <!--<SolidColorBrush x:Key="Button_Style_Icon_01.MouseOver.Background" Color="Transparent"/>-->
    <!--<SolidColorBrush x:Key="Button_Style_Icon_01.MouseOver.BorderBrush" Color="Transparent"/>-->
    <!--<SolidColorBrush x:Key="Button_Style_Icon_01.IsPressed.Foreground" Color="White"/>-->
    <!--<SolidColorBrush x:Key="Button_Style_Icon_01.IsPressed.Background" Color="Transparent"/>-->
    <!--<SolidColorBrush x:Key="Button_Style_Icon_01.IsPressed.BorderBrush" Color="Transparent"/>-->

    <Style x:Key="Button_Style_Icon_01" TargetType="{x:Type Button}">
        <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
        <Setter Property="Foreground" Value="{StaticResource Button_Style_Icon_01.Static.Foreground}"/>
        <Setter Property="Background" Value="{StaticResource Button_Style_Icon_01.Static.Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Button_Style_Icon_01.Static.BorderBrush}"/>
        <Setter Property="FontFamily" Value="{StaticResource ButtonFont_01}"/>
        <Setter Property="FontSize" Value="50"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="border"
                            Background="{TemplateBinding Background}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            SnapsToDevicePixels="true">
                        <ContentPresenter x:Name="contentPresenter"
                                          Focusable="False"
                                          RecognizesAccessKey="True"
                                          Margin="{TemplateBinding Padding}"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button_Style_Icon_01.MouseOver.Foreground}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
